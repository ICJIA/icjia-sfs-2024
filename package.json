    "nuxt": "^3.11.2",
    "vite-plugin-vuetify": "^2.0.3",
    "vue": "^3.4.22",
    "vue-router": "^4.3.0",
    "vuetify": "^3.5.15"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "eslint": "^9.0.0"
