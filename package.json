{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "yarn clear:cache && yarn scripts && rm -rf .nuxt && nuxt dev --port=8000 -o",
    "generate": "yarn clear:cache && rm -rf .nuxt && yarn scripts && nuxt generate",
    "generate:serve": "yarn clear:cache && rm -rf .nuxt && yarn scripts && nuxt generate && npx serve .output/public",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "build:serve": "yarn clear:cache && nuxt build && node .output/server/index.mjs ",
    "clear:cache": "rm -rf .nuxt .output",
    "create:pages": "node ./creators/pages.js",
    "create:news": "node ./creators/news.js",
    "create:routes": "node ./creators/generateRoutes.mjs",
    "create:siteMap": "node ./creators/generateSiteMap.mjs",
    "scripts": "yarn clear:cache && yarn create:pages && yarn create:routes && yarn create:siteMap"
  },
  "devDependencies": {
    "@nuxt/content": "2.13.2",
