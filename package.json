    "@vueuse/nuxt": "^10.9.0",
    "material-design-icons-iconfont": "^6.7.0",
    "nuxt": "^3.11.2",
    "vite-plugin-node-polyfills": "^0.21.0",
    "vite-plugin-vuetify": "^2.0.3",
    "vue": "^3.4.23",
    "vue-router": "^4.3.2",
    "vuetify": "^3.5.16"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "dotenv": "^16.4.5",
    "eslint": "^9.0.0",
    "thumbor-js-url-builder": "^2.2.0",
    "thumbor-url-builder-ts": "^0.1.9"
  }
