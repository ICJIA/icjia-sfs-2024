{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "yarn clear:cache && rm -rf .nuxt && nuxt dev --port=8000 -o",
    "generate": "nuxt generate",
    "generate:serve": "yarn clear:cache && nuxt generate && npx serve .output/public",
    "preview": "nuxt preview",
